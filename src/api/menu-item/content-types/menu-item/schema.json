{
  "kind": "collectionType",
  "collectionName": "menu_items",
  "info": {
    "singularName": "menu-item",
    "pluralName": "menu-items",
    "displayName": "MenuItem"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "order": {
      "type": "integer"
    },
    "target": {
      "type": "enumeration",
      "enum": [
        "_self",
        "_blank"
      ]
    },
    "enabled": {
      "type": "boolean"
    },
    "parent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::menu-item.menu-item",
      "inversedBy": "childrens"
    },
    "childrens": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::menu-item.menu-item",
      "mappedBy": "parent"
    },
    "isButtonLink": {
      "type": "boolean",
      "default": false
    }
  }
}
